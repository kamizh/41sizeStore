<Window x:Class="Zaripov41project.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Zaripov41project"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800" FontFamily="Tw Cen MT" Icon="res/icon.ico">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="75"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="30"/>
        </Grid.RowDefinitions>
        <Image Source="res/logo.png"  HorizontalAlignment="Left"></Image>
        <TextBlock Text="41 Размер" FontSize="30" HorizontalAlignment="Center" VerticalAlignment="Center"></TextBlock>
        <Frame Grid.Row="1" Name="MainFrame" ContentRendered="MainFrame_ContentRendered" NavigationUIVisibility="Hidden"></Frame>
        <Button Content="Назад" Name="btnBack" Width="175" Height="30" HorizontalAlignment="Right" Margin="5" Click="btnBack_Click" Background="#FFCC6600"></Button>
    </Grid>
</Window>
